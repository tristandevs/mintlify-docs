{
  "openapi": "3.0.1",
  "info": {
    "title": "OpenAPI Plant Store",
    "description": "A sample API that uses a plant store as an example to demonstrate features in the OpenAPI specification",
    "license": {
      "name": "MIT"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.shuttleai.app"
    }
  ],
  "security": [
    {
      "bearerAuth": []
    }
  ],
  "paths": {
    "/v1/models": {
      "get": {
        "description": "Returns all models from ShuttleAI",
        "security": [],
        "parameters": [
          {
            "name": "endpoints",
            "in": "query",
            "description": "Filters the models by the endpoints, (e.g /v1/images/generations)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "format",
            "in": "query",
            "description": "The format of the response > Valid options: `names`, `premium`, `free`.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Model"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/models/{id}": {
      "get": {
        "description": "Returns a single model from ShuttleAI",
        "security": [],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Model"
                }
              }
            }
          }
        }
      }
    },
    "/v1/chat/completions": {
      "post": {
        "description": "Returns a `chat.completion` object with the generated response.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "model": {
                    "description": "The id of the model to use for the generation",
                    "type": "string"
                  },
                  "messages": {
                    "type": "array",
                    "description": "The list of messages to generate a response for. The last message is the prompt.",
                    "items": {
                      "type": "object",
                      "properties": {
                        "role": {
                          "type": "string"
                        },
                        "content": {
                          "type": "string"
                        }
                      }
                    }
                  },
                  "max_tokens": {
                    "type": "integer",
                    "description": "The maximum number of tokens to generate"
                  },
                  "temperature": {
                    "type": "number",
                    "description": "The temperature of the sampling distribution"
                  },
                  "top_p": {
                    "type": "number",
                    "description": "The cumulative probability of the top tokens to keep in the nucleus of the distribution"
                  },
                  "tools": {
                    "type": "array",
                    "description": "The list of tools to use for the generation",
                    "items": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string"
                        },
                        "function": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string"
                            },
                            "parameters": {
                              "type": "object"
                            }
                          }
                        }
                      }
                    }
                  },
                  "tool_choice": {
                    "oneOf": [
                      {
                        "type": "string",
                        "description": "Defaults to `auto` for automatic tool choice"
                      },
                      {
                        "type": "object",
                        "description": "The tool choice object",
                        "properties": {
                          "name": {
                            "type": "string"
                          }
                        }
                      }
                    ]
                  },
                  "internet": {
                    "type": "boolean",
                    "description": "Whether to use the internet for the generation > Model limitations apply, defaults to `true` for bing models."
                  },
                  "citations": {
                    "type": "boolean",
                    "description": "Whether to include citations in the generation > For use in `owned_by: bing/openai` models only, defaults to `true`."
                  },
                  "tone": {
                    "type": "string",
                    "description": "The tone of the generation > For use in `owned_by: bing/openai` models only, defaults to `precise`."
                  },
                  "raw": {
                    "type": "boolean",
                    "description": "Whether to return the raw response > For use in `owned_by: bing/openai` models only! Returns Bing AI Suggestions and Search Results, defaults to `false`."
                  },
                  "image": {
                    "oneOf": [
                      {
                        "type": "string",
                        "description": "The url of the image to use for the generation > Model limitations apply"
                      },
                      {
                        "type": "boolean",
                        "description": "Automatically get image url from last message in messages provided if `true`"
                      }
                    ]
                  }
                },
                "required": [
                  "model",
                  "messages"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "object": {
                      "type": "string"
                    },
                    "created": {
                      "type": "integer"
                    },
                    "model": {
                      "type": "string"
                    },
                    "choices": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "index": {
                            "type": "integer"
                          },
                          "logprobs": {
                            "type": "object"
                          },
                          "finish_reason": {
                            "type": "string"
                          },
                          "message": {
                            "type": "object",
                            "properties": {
                              "content": {
                                "type": "string"
                              },
                              "role": {
                                "type": "string"
                              }
                            }
                          }
                        }
                      }
                    },
                    "usage": {
                      "type": "object",
                      "properties": {
                        "prompt_tokens": {
                          "type": "integer"
                        },
                        "completion_tokens": {
                          "type": "integer"
                        },
                        "total_tokens": {
                          "type": "integer"
                        }
                      }
                    },
                    "system_fingerprint": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/images/generations": {
      "post": {
        "description": "Returns a `image.generation` object with the generated image.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "model": {
                    "type": "string"
                  },
                  "prompt": {
                    "type": "string"
                  }
                },
                "required": [
                  "model",
                  "prompt"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "created": {
                      "type": "number"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "url": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/audio/generations": {
      "post": {
        "description": "Returns an `audio.generation` object with the generated audio.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "model": {
                    "type": "string"
                  },
                  "input": {
                    "type": "string"
                  },
                  "voice": {
                    "type": "string",
                    "description": "The voice to use for the generation > Voices can be found [here](https://api.shuttleai.app/v1/models)"
                  }
                },
                "required": [
                  "model",
                  "input"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "chars": {
                      "type": "integer"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "url": {
                            "type": "string"
                          }
                        }
                      }
                    },
                    "expiresIn": {
                      "type": "integer"
                    },
                    "model": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/audio/transcriptions": {
      "post": {
        "description": "Returns an `audio.transcription` object with the generated transcription.",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  },
                  "model": {
                    "type": "string",
                    "description": "Defaults to `whisper-large`. The id of the model to use for the transcription"
                  }
                },
                "required": [
                  "file"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "text": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/moderations": {
      "post": {
        "description": "Returns a `moderation` object with the analysis result.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "input": {
                    "type": "string"
                  },
                  "model": {
                    "type": "string",
                    "description": "Defaults to `text-moderation-stable`. The id of the model to use for the moderation"
                  }
                },
                "required": [
                  "input"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "model": {
                      "type": "string"
                    },
                    "result": {
                      "type": "object",
                      "properties": {
                        "flagged": {
                          "type": "boolean"
                        },
                        "categories": {
                          "type": "object"
                        },
                        "category_scores": {
                          "type": "object"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/embeddings": {
      "post": {
        "description": "Returns an `embedding` object with the embeddings.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "model": {
                    "type": "string"
                  },
                  "input": {
                    "oneOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    ]
                  }
                },
                "required": [
                  "model",
                  "input"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "object": {
                      "type": "string"
                    },
                    "data": {
                      "type": "array"
                    },
                    "model": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/joke": {
      "get": {
        "description": "Returns a random joke.",
        "parameters": [{
          "name": "n",
          "in": "query",
          "description": "Defaults to `1`. The number of jokes to return - Maximum of `4`",
          "required": false,
          "schema": {
            "type": "number"
          }
        }],
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "joke": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v1/insult": {
      "get": {
        "description": "Returns a random insult.",
        "parameters": [{
          "name": "n",
          "in": "query",
          "description": "Defaults to `1`. The number of insults to return - Maximum of `4`",
          "required": false,
          "schema": {
            "type": "integer"
          }
        }],
        "responses": {
          "200": {
            "description": "Model response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "insult": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Model": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "object": {
            "description": "The name of the model",
            "type": "string"
          },
          "data": {
            "description": "The list of model objects.",
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "object": {
                  "type": "string"
                },
                "owned_by": {
                  "type": "string"
                },
                "tokens": {
                  "type": "integer"
                },
                "cost": {
                  "type": "integer"
                },
                "premium": {
                  "type": "boolean"
                },
                "created": {
                  "type": "integer"
                }
              }
            }
          }
        }
      },
      "Error": {
        "required": [
          "error",
          "message"
        ],
        "type": "object",
        "properties": {
          "error": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}